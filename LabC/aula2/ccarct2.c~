#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int poiu(int str[]){
  
  int soma=0;
  
  for(int i=0;i!='\n';i++){
    soma++;}
  
  return soma;
}


int main(int argc, const char* argv[]) {

  int soma=0;


  for(int i=1;i!=argc;i++){
    
    for(int d=0;d!=poiu(argv[i]);d++){
      soma++;
    }
    printf("%d\n", soma);
    soma=0;
  }

  return 0;
}
